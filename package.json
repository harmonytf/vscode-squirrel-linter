{
    "name": "vscode-squirrel-linter",
    "displayName": "Respawn Squirrel Language Linter (r1)",
    "description": "For Titanfall Online and Titanfall 1. Requires Windows to work.",
    "version": "0.0.1",
    "publisher": "Harmony",
    "icon": "harmony.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.71.0"
    },
    "categories": [
        "Programming Languages",
        "Linters"
    ],
    "keywords": [
        "squirrel",
        "rsquirrel",
        "r1squirrel",
        "respawn",
        "titanfall"
    ],
    "activationEvents": [
        "onLanguage:squirrel"
    ],
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "squirrel",
                "aliases": [
                    "Squirrel",
                    "squirrel",
                    "rsquirrel",
                    "r1squirrel"
                ],
                "extensions": [
                    ".nut"
                ]
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/harmonytf/vscode-squirrel-linter"
    },
    "devDependencies": {
        "@types/node": "^18.7.18",
        "@types/vscode": "^1.71.0"
    }
}
